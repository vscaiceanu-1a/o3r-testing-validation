{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/spec",
    "types": [
      "jasmine"
    ],
    "baseUrl": ".",
    "lib": [
      "dom",
      "es2020",
      "scripthost"
    ],
    "paths": {
      "@o3r/testing/core": [
        "node_modules/@o3r/testing/core/angular"
      ],
      "@o3r/testing/core/*": [
        "node_modules/@o3r/testing/core/angular/*"
      ]
    }
  },
  "include": [
    "src/**/*.spec.ts",
    "src/**/*.d.ts"
  ]
}